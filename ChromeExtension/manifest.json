{
  "manifest_version": 2,
  "name": "URL Extractor",
  "version": "1.0",
  "description": "Capture current URL and send to Flask server",
  "background": {
    "scripts": ["background.js"]
  },
  "browser_action": {
    "default_popup": "popup.html"
  },
  "permissions": ["activeTab", "http://localhost:5000/*","tabs"]
}
