docker pull f80hub/shortlinks:latest
docker ps -q --filter "name=shortlinks" | grep -q .
docker rm -f shortlinks
docker run --name shortlinks --restart=always -e SERVICES_PATH=https://linkut.f80.fr/assets/services.yaml -p 444:444 -e PORT=444 -e SSL=True -e DBPATH=cloud -d f80hub/shortlinks:latest
exit


